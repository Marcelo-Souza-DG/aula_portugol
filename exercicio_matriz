programa
{
	
	funcao inicio()
	{
		inteiro contador, limite
		contador=0
		limite=2
		
		cadeia referencia [][]={{"João","São Paulo","(11) 9999-5241"},{"Maria","Ribeirão Preto","(16) 9999-8596"},{"Ana","Manaus","(92) 9999-8574"}}

		faca{
			escreva(" MORADOR: " + referencia[contador][0] + " LOCAL DE RESIDÊNCIA: " + referencia[contador][1] + " TELEFONE Nº: " + referencia[contador][2] + "\n")
			contador++
			
		}enquanto(contador<=2)
		
	}	
}
